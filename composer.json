{
  "name": "imajinyun/php-design-patterns",
  "description": "An example of design pattern implemented in PHP language.",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/imajinyun/php-design-patterns/issues"
  },
  "authors": [
    {
      "name": "imajinyun",
      "email": "imajinyun@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-xdebug": "*",
    "psr/http-message": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "DesignPattern\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DesignPattern\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
